{% extends 'base.html.twig' %}

{% block title %}{{ title }}{% endblock %}

{% block body %}

            <h1>Matched Members</h1>
    {{ form(filter_form) }}
                {% for match in matches %}
                    <div>{{ match.name }}
                        <ul>
                            <li>Amžius: {{ match.age }}</li>
                            <li>Marsrutas: {{ match.route }}</li>
                            <li>Atstumas tarp gyvenamu vietų: {{ match.home_distance_text }}</li>
                            <li>Atstumas tarp darbo vietų: {{ match.work_distance_text }}</li>
                            <li>Apie: {{ match.description }}</li>
                        </ul>
                    </div>


                {% endfor %}


{% endblock %}